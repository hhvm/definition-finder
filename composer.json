{
  "name": "facebook/definition-finder",
  "description": "Find definitions in PHP or Hack files. Useful for autoloaders.",
  "keywords": ["hack", "definitions", "hhvm", "autoload"],
  "homepage": "https://github.com/hhvm/definitions-finder",
  "license": "MIT",
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  },
  "require": {
    "hhvm": "^4.15",
    "hhvm/hhvm-autoload": "^2.0",
    "hhvm/hsl": "^4.0",
    "hhvm/type-assert": "^3.2",
    "hhvm/hhast": "^4.21.4"
  },
  "require-dev": {
    "facebook/fbexpect": "^2.0.0",
    "hhvm/hacktest": "^1.0"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "files": [ "autoload_files.php" ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  }
}
